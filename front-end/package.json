    "class-transformer": "^0.5.1",
    "element-plus": "^2.8.7",
    "moment": "^2.30.1",
    "pinia": "^2.2.6",
    "pinia-plugin-persistedstate": "^4.1.3",
    "reflect-metadata": "^0.2.2",
    "tsyringe": "^4.8.0",
    "vue": "^3.5.12",
    "vue-router": "^4.4.5",
    "vuex": "^4.1.0"
